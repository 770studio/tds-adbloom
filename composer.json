{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4.0|^8.0",
        "ext-json": "*",
        "bilaleren/tune-api": "^1.0",
        "digital-creative/clickable-table-row": "^0.2.0",
        "digital-creative/collapsible-resource-manager": "^1.3",
        "ebess/nova-collapsible-sidebar": "^0.1.2",
        "epartment/nova-dependency-container": "^1.3",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.3",
        "hidehalo/nanoid-php": "^1.1",
        "laravel/framework": "^v8.47.0",
        "laravel/horizon": "^5.7",
        "laravel/nova": "*",
        "laravel/slack-notification-channel": "^2.3",
        "laravel/tinker": "^2.5",
        "naif/toggle": "^1.2",
        "naoray/nova-json": "^1.3",
        "optimistdigital/nova-multiselect-field": "^2.3",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.10",
        "facade/ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "kitloong/laravel-migrations-generator": "^4.5",
        "laravel/telescope": "^4.6",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.6",
        "orangehill/iseed": "^3.0",
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },

    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ]
}
